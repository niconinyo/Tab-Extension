{
    "manifest_version": 3,
    "name": "Tab Declutterer",
    "version": "1.0.0",
    "description": "Track open tabs, bookmark them after a set time, and then close them.",
    "permissions": [
      "tabs",
      "bookmarks",
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  